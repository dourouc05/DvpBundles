        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="generator" content="DvpTeamBundle" />
        <meta name="robots" content="index, follow" />
        <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
        <link rel="image_src" href="http://www.developpez.com/facebook-icon.png" />
        <link rel="stylesheet" type="text/css" href="/template/gabarit0.css" />
        <link rel="stylesheet" type="text/css" media="print" href="http://www.developpez.com/template/printer.css" />
        <link rel="stylesheet" type="text/css" media="screen" href="http://www.developpez.com/public/css/index2.css" />
        <link rel="stylesheet" type="text/css" media="screen" href="http://www.developpez.com/template/kit/article.css" />
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('bundles/dvpteam/index.css') }}" />
