    <body xml:lang="fr">
        {{ 65|gab_up }}
        {% if section.rightColumn %}
		<table>
			<tr>
				<td valign="top" style="width: 100%; ">
                    <div class="divarticle">
        {% endif %}
        
        <div id="teamPage">
            <div id="container">
                <div id="header">
                    <div id="logo" style="text-align: center">
                        <img src="{{ section.image }}" alt="Logo"/>
                    </div>
                    <br/><br/><br/><br/><br/><br/>
                    {{ section.text|raw }}
                    <p class="presentation" style="text-align:center; padding-top: 1px">
                        {% for category in categories %}
                            {% if category.getMembers(section) %}
                                {% include 'DvpTeamBundle:Default:headerList.html.twig' with {'category': category, 'section': section} only %}
                            {% endif %}
                        {% endfor %}
                    </p>
                </div>
                <div id="membres">
                    <table style="width: 100%;">
                        {% for category in categories %}
                            {% include 'DvpTeamBundle:Default:list.html.twig' with {'category': category, 'section': section} only %}
                        {% endfor %}
                    </table>
                </div>
            </div>
        </div>
        
        {% if section.rightColumn %}
                    </div>
				</td>
				<td style="width: 300px; " valign="top">
                    {{ section.rightColumn|php_include|convert_encoding('ISO-8859-1', 'UTF-8')|raw }}
				</td>
			</tr>
		</table>
        {% endif %}
        
        {{ 65|gab_down(true) }}
    </body>
